{
  "name": "battle-rivl",
  "version": "1.0.0",
  "description": "Simple score tracking system",
  "main": "./src/index.jsx",
  "scripts": {
    "start": "NODE_ENV=development node server/index.js",
    "resetdb": "node server/resetdb.js",
    "clean-public": "npm run remove-public && mkdir public",
    "remove-public": "node_modules/.bin/rimraf ./public",
    "build:html": "babel-node tools/buildHtml.js",
    "build:images": "cp -R dist/images/ public/images",
    "prebuild": "npm-run-all clean-public build:html build:images",
    "heroku-prebuild": "npm-run-all clean-public build:html build:images",
    "build": "babel-node tools/build.js",
    "postbuild": "NODE_ENV=production babel-node server/index.js",
    "heroku-postbuild": "NODE_ENV=production babel-node server/index.js"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "rethinkdb"
  ],
  "engines": {
    "node": "4.6.2"
  },
  "author": "Rob Feng",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.17.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.25.0",
    "node-sass": "^3.11.2",
    "npm-run-all": "^4.0.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "npm-run-all": "^4.0.1",
    "bluebird": "^3.4.6",
    "elo-js": "^1.0.1",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.16.4",
    "material-ui": "^0.16.1",
    "node-uuid": "^1.4.7",
    "normalizr": "^2.2.1",
    "react-crop": "^4.0.2",
    "react-rethinkdb": "^0.6.0",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux-logger": "^2.7.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "rethinkdb-websocket-client": "^0.5.1",
    "rethinkdb-websocket-server": "^0.6.0"
  }
}
